<MyWidget>:
	rows:2
	spacing:10
	padding:10
	height: self.minimum_height
	id: grid1
	GridLayout:
		rows:1
		size_hint_y: 0.05
		spacing:3
	
		Button:
			dropdown:dropdown.__self__
			id:AlgoButton
			background_color: 0.1, 0.5, 0.6, 1
		    text: 'Algo'
			size_hint_y: 0.05
			color: .8,.9,0,1
			on_parent: dropdown.dismiss()
        	on_release: dropdown.open(self)

		DropDown:
			id: dropdown
	        on_select: AlgoButton.text = '{}'.format(args[1])
	        
	        Button:

	        	background_color: 0.1, 0.5, 0.6, 1
			    text: 'BFS'
				size_hint_y:None
				height:30
				color: .8,.9,0,1
				on_release: dropdown.select('BFS')
			Button:

	        	background_color: 0.1, 0.5, 0.6, 1
			    text: 'DFS'
				size_hint_y:None
				height:30
				color: .8,.9,0,1
				on_release: dropdown.select('DFS')

			Button:

	        	background_color: 0.1, 0.5, 0.6, 1
			    text: 'A*'
				size_hint_y:None
				height:30
				color: .8,.9,0,1
				on_release: dropdown.select('A*')

			Button:

	        	background_color: 0.1, 0.5, 0.6, 1
			    text: "Dijikstra's"
				size_hint_y:None
				height:30
				color: .8,.9,0,1
				on_release: dropdown.select("Dijikstra's")

			Button:

	        	background_color: 0.1, 0.5, 0.6, 1
			    text: 'All-Paths-DFS'
				size_hint_y:None
				height:30
				color: .8,.9,0,1
				on_release: dropdown.select('All-Paths-DFS')

			Button:

	        	background_color: 0.1, 0.5, 0.6, 1
			    text: 'IDS'
				size_hint_y:None
				height:30
				color: .8,.9,0,1
				on_release: dropdown.select('IDS')


		
		Button:
			dropdown_srcX:dropdown_srcX.__self__
			id:SourceX			
			background_color: 0.1, 0.5, 0.6, 1
		    text: 'Src-x: NA'
			size_hint_y: 0.05
			color: .8,.9,0,1
			on_parent: dropdown_srcX.dismiss()
        	on_release: dropdown_srcX.open(self)

        DropDown:
        	id:dropdown_srcX
        	on_select: SourceX.text = '{}'.format(args[1])    	

        	TextInput:
	            id:srcxinp
	            multiline:False
	            background_color: 0, 0.25, 0.35, 1
	            cursor_color: .8,.9,0,1
	            foreground_color:.8,.9,0,1
	            input_filter:"int"
	            size_hint_y: None
	        	height:30
	        	hint_text:'Enter'
	        	on_focus: root.Input()
			
		Button:
			dropdown_srcY:dropdown_srcY.__self__
			id:SourceY			
			background_color: 0.1, 0.5, 0.6, 1
		    text: 'Src-y: NA'
			size_hint_y: 0.05
			color: .8,.9,0,1
			on_parent: dropdown_srcY.dismiss()
        	on_release: dropdown_srcY.open(self)

        DropDown:
        	id:dropdown_srcY
        	on_select: SourceY.text = '{}'.format(args[1])
        	

        	TextInput:
	            id:srcyinp
	            multiline:False
	            background_color: 0, 0.25, 0.35, 1
	            cursor_color: .8,.9,0,1
	            foreground_color:.8,.9,0,1
	            input_filter:"int"
	            size_hint_y:None
	        	height:30
	        	on_focus: root.Input()
	        	hint_text:'Enter'
		    	
		Button:
			dropdown_destX:dropdown_destX.__self__
			id:DestX			
			background_color: 0.1, 0.5, 0.6, 1
		    text: 'Dest-x: NA'
			size_hint_y: 0.05
			color: .8,.9,0,1
			on_parent: dropdown_destX.dismiss()
        	on_release: dropdown_destX.open(self)

        DropDown:
        	id:dropdown_destX
        	on_select: DestX.text = '{}'.format(args[1])
        	

        	TextInput:
	            id:destxinp
	            multiline:False
	            background_color: 0, 0.25, 0.35, 1
	            cursor_color: .8,.9,0,1
	            foreground_color:.8,.9,0,1
	            input_filter:"int"
	            size_hint_y:None
	        	height:30
		    	on_focus: root.Input()
		    	hint_text:'Enter'
		

		Button:
			dropdown_destY:dropdown_destY.__self__
			id:DestY			
			background_color: 0.1, 0.5, 0.6, 1
		    text: 'Dest-y: NA'
			size_hint_y: 0.05
			color: .8,.9,0,1
			on_parent: dropdown_destY.dismiss()
        	on_release: dropdown_destY.open(self)

        DropDown:
        	id:dropdown_destY
        	on_select: DestY.text = '{}'.format(args[1])
        	

        	TextInput:
	            id:destyinp
	            multiline:False
	            background_color: 0, 0.25, 0.35, 1
	            cursor_color: .8,.9,0,1
	            foreground_color:.8,.9,0,1
	            input_filter:"int"
	            size_hint_y:None
	        	height:30
	        	hint_text:'Enter'
	        	on_focus: root.Input()



		Button:

			id:Start
			
			background_color: 0, 1, 0, 1
		    text: 'Start'
			size_hint_y: 0.05
			color: .8,.9,0,1
			on_press:root.start()



		Button:
			
			id:Reset
			background_color: 1, 0, 0, 1
			text: 'Reset'
			size_hint_y: 0.05
			color: .8,.9,0,1
			on_press:root.reset()


		Button:
			
			id:About
			background_color: 0, 0, 1, 1
		    text: 'About'
			size_hint_y: 0.05
			color: .8,.9,0,1

	
	GridLayout:
		id:grid2
		spacing:1
		padding:1

		


	
		
		